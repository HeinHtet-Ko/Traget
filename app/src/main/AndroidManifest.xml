<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mtu.ceit.hhk.traget">

<uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>

    <application
        android:name=".TragetApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_small_tractor"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_small_tractor"
        android:supportsRtl="true"
        android:theme="@style/Theme.Traget">
        <activity android:name=".features.MainActivity"

            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>

</manifest>